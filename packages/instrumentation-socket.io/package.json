{
    "name": "opentelemetry-instrumentation-socket.io",
    "version": "0.4.0",
    "description": "open telemetry instrumentation for the `socket.io` module",
    "keywords": [
        "socket.io",
        "opentelemetry"
    ],
    "author": {
        "name": "Aspecto",
        "email": "support@aspecto.io",
        "url": "https://aspecto.io"
    },
    "scripts": {
        "build": "tsc",
        "prepare": "yarn run build",
        "test": "mocha",
        "test:ci": "yarn test",
        "watch": "tsc -w",
        "version:update": "node ../../scripts/version-update.js",
        "version": "yarn run version:update"
    },
    "homepage": "https://github.com/aspecto-io/opentelemetry-ext-js",
    "license": "Apache-2.0",
    "main": "dist/src/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/aspecto-io/opentelemetry-ext-js.git"
    },
    "bugs": {
        "url": "https://github.com/aspecto-io/opentelemetry-ext-js/issues"
    },
    "dependencies": {
        "@opentelemetry/api": "^1.0.0-rc.0",
        "@opentelemetry/instrumentation": "^0.19.0",
        "@opentelemetry/semantic-conventions": "^0.19.0"
    },
    "devDependencies": {
        "socket.io": "^4.0.2",
        "socket.io-client": "^4.1.1"
    },
    "mocha": {
        "extension": [
            "ts"
        ],
        "spec": "test/**/*.spec.ts",
        "require": "ts-node/register"
    }
}
