{
    "name": "opentelemetry-plugin-aws-sdk",
    "version": "0.1.1",
    "description": "open telemetry instrumentation for the `aws-sdk` package",
    "keywords": [
        "aws",
        "opentelemetry",
        "aws-sdk"
    ],
    "homepage": "https://github.com/aspecto-io/opentelemetry-ext-js",
    "license": "Apache-2.0",
    "main": "dist/src/index.js",
    "files": [
        "dist/src/**/*.js",
        "dist/src/**/*.d.ts",
        "LICENSE",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/aspecto-io/opentelemetry-ext-js.git"
    },
    "scripts": {
        "build": "tsc",
        "prepare": "yarn run build",
        "test": "jest",
        "watch": "tsc -w"
    },
    "bugs": {
        "url": "https://github.com/aspecto-io/opentelemetry-ext-js/issues"
    },
    "devDependencies": {
        "@opentelemetry/api": "^0.14.0",
        "@opentelemetry/node": "^0.14.0",
        "@opentelemetry/tracing": "^0.14.0",
        "@types/jest": "^26.0.15",
        "@types/shimmer": "^1.0.1",
        "aws-sdk": "^2.780.0",
        "jest": "^26.6.1",
        "ts-jest": "^26.4.3",
        "typescript": "^4.0.5"
    },
    "dependencies": {
        "@opentelemetry/api": "^0.14.0",
        "@opentelemetry/core": "^0.14.0",
        "@opentelemetry/semantic-conventions": "^0.14.0",
        "opentelemetry-propagation-utils": "0.1.0",
        "shimmer": "^1.2.1"
    },
    "jest": {
        "preset": "ts-jest",
        "globals": {
            "ts-jest": {
                "tsconfig": "./tsconfig.json"
            }
        },
        "testMatch": [
            "**/test/**/*.spec.+(ts|tsx)"
        ],
        "testEnvironment": "node"
    }
}
