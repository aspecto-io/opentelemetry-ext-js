{
    "name": "opentelemetry-instrumentation-express",
    "version": "0.4.2",
    "description": "enhanced open telemetry instrumentation for the `express` web framework",
    "author": "Aspecto.io",
    "license": "Apache-2.0",
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.js",
    "files": [
        "dist/src/**/*.js",
        "dist/src/**/*.d.ts",
        "LICENSE",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aspecto-io/aspecto-opentelemetry-js.git"
    },
    "scripts": {
        "build": "tsc",
        "prepare": "yarn run build",
        "test": "mocha",
        "watch": "tsc -w",
        "version:update": "node ../../scripts/version-update.js",
        "version": "yarn run version:update",
        "test-all-versions": "tav",
        "test:ci": "yarn test-all-versions"
    },
    "publishConfig": {
        "access": "public"
    },
    "peerDependencies": {
        "@opentelemetry/api": "1.0.0-rc.0"
    },
    "dependencies": {
        "@opentelemetry/instrumentation": "^0.19.0",
        "@opentelemetry/semantic-conventions": "^0.19.0"
    },
    "devDependencies": {
        "@opentelemetry/api": "1.0.0-rc.0",
        "@opentelemetry/instrumentation-http": "^0.19.0",
        "@opentelemetry/tracing": "^0.19.0",
        "@types/express": "4.17.8",
        "@types/mocha": "^8.2.2",
        "axios": "0.21.1",
        "body-parser": "^1.19.0",
        "expect": "^26.6.2",
        "express": "4.17.1",
        "mocha": "^8.3.2",
        "test-all-versions": "^5.0.1"
    },
    "mocha": {
        "extension": [
            "ts"
        ],
        "spec": "test/**/*.spec.ts",
        "require": "ts-node/register"
    }
}
