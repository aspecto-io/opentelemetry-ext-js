{
    "name": "opentelemetry-instrumentation-express",
    "version": "0.22.0",
    "description": "enhanced open telemetry instrumentation for the `express` web framework",
    "author": "Aspecto.io",
    "license": "Apache-2.0",
    "main": "dist/src/index.js",
    "types": "dist/src/index.d.js",
    "files": [
        "dist/src/**/*.js",
        "dist/src/**/*.d.ts",
        "dist/src/**/*.js.map",
        "LICENSE",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/aspecto-io/opentelemetry-ext-js.git"
    },
    "scripts": {
        "build": "tsc",
        "prepare": "yarn run build",
        "test": "mocha --require opentelemetry-instrumentation-testing-utils",
        "test:jaeger": "OTEL_EXPORTER_JAEGER_AGENT_HOST=localhost mocha --require opentelemetry-instrumentation-testing-utils",
        "watch": "tsc -w",
        "version:update": "node ../../scripts/version-update.js",
        "version": "yarn run version:update",
        "test-all-versions": "tav",
        "test:ci": "yarn test-all-versions"
    },
    "publishConfig": {
        "access": "public"
    },
    "peerDependencies": {
        "@opentelemetry/api": "^1.0.0"
    },
    "dependencies": {
        "@opentelemetry/core": "^0.22.0",
        "@opentelemetry/instrumentation": "^0.22.0",
        "@opentelemetry/semantic-conventions": "^0.22.0",
        "parseurl": "~1.3.3",
        "path-to-regexp": "0.1.7"
    },
    "devDependencies": {
        "@opentelemetry/api": "^1.0.0",
        "@opentelemetry/instrumentation-http": "^0.22.0",
        "@opentelemetry/tracing": "^0.22.0",
        "@types/express": "4.17.8",
        "@types/mocha": "^8.2.2",
        "axios": "0.21.1",
        "body-parser": "^1.19.0",
        "expect": "^26.6.2",
        "express": "4.17.1",
        "mocha": "^8.4.0",
        "opentelemetry-instrumentation-mocha": "0.0.1-rc.3",
        "opentelemetry-instrumentation-testing-utils": "^0.22.0",
        "test-all-versions": "^5.0.1",
        "typescript": "4.3.4"
    },
    "mocha": {
        "extension": [
            "ts"
        ],
        "spec": "test/**/*.spec.ts",
        "require": [
            "ts-node/register",
            "opentelemetry-instrumentation-mocha"
        ]
    }
}
