{
    "name": "opentelemetry-instrumentation-aws-sdk",
    "version": "0.4.0",
    "description": "open telemetry instrumentation for the `aws-sdk` package",
    "keywords": [
        "aws",
        "opentelemetry",
        "aws-sdk"
    ],
    "homepage": "https://github.com/aspecto-io/opentelemetry-ext-js",
    "license": "Apache-2.0",
    "main": "dist/src/index.js",
    "files": [
        "dist/src/**/*.js",
        "dist/src/**/*.d.ts",
        "LICENSE",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/aspecto-io/opentelemetry-ext-js.git"
    },
    "scripts": {
        "build": "tsc",
        "prepare": "yarn run build",
        "test": "mocha",
        "test:ci": "yarn test-all-versions",
        "watch": "tsc -w",
        "version:update": "node ../../scripts/version-update.js",
        "version": "yarn run version:update",
        "test-all-versions": "tav"
    },
    "bugs": {
        "url": "https://github.com/aspecto-io/opentelemetry-ext-js/issues"
    },
    "dependencies": {
        "@opentelemetry/api": "^1.0.0-rc.0",
        "@opentelemetry/instrumentation": "^0.19.0",
        "@opentelemetry/semantic-conventions": "^0.19.0",
        "opentelemetry-propagation-utils": "^0.6.0"
    },
    "devDependencies": {
        "@aws-sdk/client-s3": "3.13.1",
        "@aws-sdk/client-sqs": "3.13.1",
        "@aws-sdk/types": "3.13.1",
        "@opentelemetry/node": "^0.19.0",
        "@opentelemetry/tracing": "^0.19.0",
        "@types/mocha": "^8.2.0",
        "aws-sdk": "^2.780.0",
        "expect": "^26.6.2",
        "mocha": "^8.3.0",
        "nock": "^13.0.11",
        "test-all-versions": "^5.0.1",
        "ts-node": "^9.1.1",
        "typescript": "^4.0.5"
    },
    "mocha": {
        "extension": [
            "ts"
        ],
        "spec": "test/**/*.spec.ts",
        "require": "ts-node/register"
    }
}
