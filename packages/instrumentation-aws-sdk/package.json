{
    "name": "opentelemetry-instrumentation-aws-sdk",
    "version": "0.24.1",
    "description": "open telemetry instrumentation for the `aws-sdk` package",
    "keywords": [
        "aws",
        "opentelemetry",
        "aws-sdk"
    ],
    "homepage": "https://github.com/aspecto-io/opentelemetry-ext-js",
    "license": "Apache-2.0",
    "main": "dist/src/index.js",
    "files": [
        "dist/src/**/*.js",
        "dist/src/**/*.d.ts",
        "dist/src/**/*.js.map",
        "LICENSE",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/aspecto-io/opentelemetry-ext-js.git"
    },
    "scripts": {
        "build": "tsc",
        "prepare": "yarn run build",
        "test": "mocha --require opentelemetry-instrumentation-testing-utils",
        "test:jaeger": "OTEL_EXPORTER_JAEGER_AGENT_HOST=localhost mocha --require opentelemetry-instrumentation-testing-utils",
        "test:ci": "yarn test-all-versions",
        "watch": "tsc -w",
        "version:update": "node ../../scripts/version-update.js",
        "version": "yarn run version:update",
        "test-all-versions": "tav"
    },
    "bugs": {
        "url": "https://github.com/aspecto-io/opentelemetry-ext-js/issues"
    },
    "peerDependencies": {
        "@opentelemetry/api": "^1.0.1"
    },
    "dependencies": {
        "@opentelemetry/core": "^0.24.0",
        "@opentelemetry/instrumentation": "^0.24.0",
        "@opentelemetry/semantic-conventions": "^0.24.0",
        "opentelemetry-propagation-utils": "^0.24.0"
    },
    "devDependencies": {
        "@aws-sdk/client-s3": "3.13.1",
        "@aws-sdk/client-sqs": "3.13.1",
        "@aws-sdk/types": "3.13.1",
        "@opentelemetry/api": "^1.0.2",
        "@opentelemetry/tracing": "^0.24.0",
        "@types/mocha": "^8.2.2",
        "@types/node": "^14.0.0",
        "aws-sdk": "^2.780.0",
        "expect": "^26.6.2",
        "mocha": "^8.4.0",
        "nock": "^13.0.11",
        "opentelemetry-instrumentation-mocha": "0.0.1-rc.5",
        "opentelemetry-instrumentation-testing-utils": "^0.24.0",
        "test-all-versions": "^5.0.1",
        "ts-node": "^9.1.1",
        "typescript": "4.3.4"
    },
    "mocha": {
        "extension": [
            "ts"
        ],
        "spec": "test/**/*.spec.ts",
        "require": [
            "ts-node/register",
            "opentelemetry-instrumentation-mocha"
        ]
    }
}
